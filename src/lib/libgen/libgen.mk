install: ALL
	$(INS) $(LIBRARY) $(USRLIB)/$(LIBRARY)
	$(AR) ts $(USRLIB)/$(LIBRARY) > /dev/null
	chmod 444 $(USRLIB)/$(LIBRARY)
	chgrp bin $(USRLIB)/$(LIBRARY)
	chown bin $(USRLIB)/$(LIBRARY)
	-@for i in $(INCFILE) $(LOCALINCS) ;\
	do \
	if [ -d $(ROOT)/usr/$(SGS)include ] ;\
	then \
		echo "$(INS) $$i $(ROOT)/usr/$(SGS)include/$$i" ;\
		$(INS) $$i $(ROOT)/usr/$(SGS)include/$$i ;\
		chmod 0444 $(ROOT)/usr/$(SGS)include/$$i ;\
		chgrp bin $(ROOT)/usr/$(SGS)include/$$i ;\
		chown bin $(ROOT)/usr/$(SGS)include/$$i ;\
	else \
		echo "$(INS) $$i $(ROOT)/usr/include/$$i" ;\
		$(INS) $$i $(ROOT)/usr/include/$$i ;\
		chmod 0444 $(ROOT)/usr/include/$$i ;\
		chgrp bin $(ROOT)/usr/include/$$i ;\
		chown bin $(ROOT)/usr/include/$$i ;\
	fi; done


